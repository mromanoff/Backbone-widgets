{
  "name": "BackboneWidgets",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www"
  },
  "dependencies": {
    "underscore": "1.6.0",
    "backbone": "^1.1.2",
    "backbone.marionette": "^2.4.1",
    "backbone.radio": "^0.9.0",
    "body-parser": "~1.12.0",
    "bootstrap-sass": "^3.3.4",
    "cookie-parser": "~1.3.4",
    "debug": "~2.1.1",
    "express": "~4.12.2",
    "handlebars": "^3.0.1",
    "hbs": "~2.9.0",
    "jquery": "^2.1.3",
    "morgan": "~1.5.1",
    "serve-favicon": "~2.2.0"
  },
  "devDependencies": {
    "browserify": "^9.0.7",
    "browserify-istanbul": "^0.2.1",
    "browserify-shim": "^3.8.3",
    "browserify-versionify": "^1.0.4",
    "del": "^1.1.1",
    "eslint": "^0.18.0",
    "eslint-plugin-backbone": "^1.0.2",
    "exorcist": "^0.1.6",
    "faker": "^2.1.2",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-concat": "^2.5.2",
    "gulp-eslint": "^0.8.0",
    "gulp-karma": "0.0.4",
    "gulp-minify-css": "^1.0.0",
    "gulp-plumber": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^1.3.3",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.4",
    "gulp-watch": "^4.2.3",
    "hbsfy": "^2.2.1",
    "jasmine-core": "^2.2.0",
    "karma": "^0.12.31",
    "karma-browserify": "^4.1.2",
    "karma-coverage": "^0.3.1",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vinyl-transform": "^1.0.0",
    "watchify": "^3.1.0"
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap.js",
    "templates/helpers": "./src/common/templates/helpers.js",
    "templates/partials": "./src/common/templates/partials.js",

    "helpers/format": "./src/common/helpers/format.js",
    "helpers/i18n": "./src/common/helpers/i18n.js",
    "helpers/locale": "./src/common/helpers/locale.js",

    "core/application": "./src/common/core/application.js",
    "core/collection": "./src/common/core/collection.js",
    "core/item-view": "./src/common/core/item-view.js",
    "core/composite-view": "./src/common/core/composite-view.js",
    "core/layout-view": "./src/common/core/layout-view.js",
    "core/model": "./src/common/core/model.js",
    "core/module": "./src/common/core/module.js",
    "config/api": "./src/config/api.js"
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "external": "global:External"
  },

  "browserify": {
    "transform": [
      [
        "hbsfy",
        {
          "extensions": [
            "hbs"
          ]
        }
      ],
      "browserify-shim",
      "browserify-versionify"
    ]
  }
}
